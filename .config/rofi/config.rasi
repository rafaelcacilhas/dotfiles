configuration {
	theme: "~/.config/rofi/themes/Nord.rasi";
	font: "Sauce Code Pro Nerd Font 9";
	modi: "drun,window,run";
	combi-modi: "drun, window";
	kb-mode-next: "Shift+Right";
	kb-mode-previous: "Shift+Left";
	show-icons: true;
	drun-display-format: "{name}";
	width: 40;
	columns: 2;
	lines: 15;
	fixed-num-lines: true;
	location: 0;
	yoffset: 0;
	sidebar-mode: false;
	fullscreen: false;
}

